package com.example.demo.service;

import java.util.List;
import java.util.Optional;

import com.example.demo.model.Address;
import com.example.demo.model.User;

public interface UserService<User> {
	
	public Optional<User> createUser(User email);
	public Optional<Address> createUser(User email, Address address);
	public Optional getUser();
	public Optional<User> Update(User u, long id);
	public Object fetchById(Object id);
}
